{
  // Multi-line settings. These can't be automatically sorted :(
  "[git-commit]": {
    "editor.wordWrap": "off",
  },
  "[git-rebase]": {
    "editor.wordWrap": "off",
  },
  "[markdown]": {
    "rewrap.autoWrap.enabled": false,
  },
  "[plaintext]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4,
    "files.insertFinalNewline": false,
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop",
  },
  "[stylus]": {
    "editor.wordWrap": "off",
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },

  "blockhighlight.background": [
    "200",
    "100",
    "255",
    ".2",
  ],
  "blockhighlight.omit": [
    "typescriptreact",
    "plaintext",
  ],
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "css",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pub",
    "python",
    "restructuredtext",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml",
    "ruby",
  ],
  "cSpell.ignoreRegExpList": [],
  "cSpell.userWords": [
    "Asana",
    "CEOs",
    "CSVs",
    "Citus",
    "Crypted",
    "Dedupe",
    "Deduping",
    "Deno",
    "Kuang",
    "Linkedin",
    "Lyft",
    "Menlo",
    "Zhang",
    "Zhou",
    "abcdefghijklmnopqrstuvwxyz",
    "anonymize",
    "architected",
    "bezier",
    "bignum",
    "bools",
    "camelization",
    "camelize",
    "csrf",
    "debounced",
    "debouncing",
    "deduped",
    "denormalize",
    "depluralize",
    "divs",
    "droppable",
    "eightvc",
    "elif",
    "encodeji",
    "eqnull",
    "esversion",
    "eval",
    "evcs",
    "expnd",
    "gabe",
    "gmail",
    "groupby",
    "gsub",
    "hacky",
    "haml",
    "hellip",
    "icomoon",
    "introducee",
    "isatty",
    "lapras",
    "linkify",
    "linkifyjs",
    "lunala",
    "mktemp",
    "munge",
    "nbsp",
    "nocheck",
    "nokogiri",
    "nums",
    "onboarded",
    "opps",
    "orgs",
    "parallelize",
    "prettierrc",
    "rbenv",
    "rc",
    "rebases",
    "reenabled",
    "regioned",
    "repo",
    "repr",
    "rgba",
    "rohan",
    "rstrip",
    "rsync",
    "rubo",
    "rubocop",
    "serializable",
    "setex",
    "shrey",
    "sidekiq",
    "spammy",
    "sscs",
    "styl",
    "tada",
    "ucomm",
    "uncomment",
    "ungated",
    "ungrouped",
    "unhide",
    "unindent",
    "unintuitive",
    "uninvitable",
    "unioned",
    "unlisten",
    "unmount",
    "unmounts",
    "unnests",
    "unpersistent",
    "unsanitized",
    "unstaged",
    "upcase",
    "vals",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
  },
  "editor.rulers": [
    90,
  ],
  "files.associations": {
    "*.sublime-settings": "jsonc",
    // "*.json": "jsonc"
  },
  "gitlens.menus": {
    "editor": false,
    "editorGroup": {
      "blame": true,
      "compare": true,
    },
    "editorTab": false,
    "explorer": false,
    "scmGroup": false,
    "scmItem": false,
  },
  "quickOpenRelatedFiles.patternsToStrip": [
    "{EXTENSION}",
    "/[-_\\.]?[sS]pec$/",
    "/[-_\\.]?[tT]est$/",
    "/[-_\\.]?[cC]omponent$/",
    "/[-_\\.]?[cC]ontainer$/",
  ],
  "ruby.lint": {
    "fasterer": true,
    "reek": true,
    // "rubocop": true,
  },
  // "workbench.colorCustomizations": {
  //   "scrollbarSlider.background": "#9aa1",
  //   "scrollbarSlider.activeBackground": "#9aac",
  //   "scrollbarSlider.hoverBackground": "#9aa8",
  // },

  // Single-line settings.
  "auto-comment-blocks.singleLineBlockOnEnter": true,
  "blockhighlight.isWholeLine": true,
  "bookmarks.backgroundLineColor": "rgba(21, 126, 251, 0.4)",
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.showCommandsInContextMenu": false,
  "bookmarks.sideBar.expanded": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.checkLimit": 30,
  "cSpell.showStatus": false,
  "debug.enableAllHovers": true,
  "debug.inlineValues": true,
  "debug.toolBarLocation": "docked",
  "deno.alwaysShowStatus": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoIndent": false,
  "editor.codeActionsOnSaveTimeout": 7000,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.globalFindClipboard": true,
  "editor.find.seedSearchStringFromSelection": false,
  "editor.foldingStrategy": "auto",
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.formatOnSaveTimeout": 5000,
  "editor.glyphMargin": true,
  "editor.gotoLocation.multiple": "gotoAndPeek",
  "editor.hideCursorInOverviewRuler": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "interval",
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 90,
  "editor.minimap.scale": 1,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "left",
  "editor.renderIndentGuides": false,
  "editor.renderWhitespace": "selection",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "visible",
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSet": "defaultOutline",
  "eslint.autoFixOnSave": true,
  "explorer.autoReveal": false,
  "explorer.decorations.badges": false,
  "explorer.sortOrder": "mixed",
  "extensions.autoUpdate": true,
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autoRepositoryDetection": false,
  "git.countBadge": "off",
  "git.enableSmartCommit": true,
  "git.openDiffOnClick": false,
  "git.showPushSuccessNotification": true,
  "gitlens.blame.compact": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.defaultGravatarsStyle": "identicon",
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.hovers.changesDiff": "hunk",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.recentChanges.highlight.locations": [
    "gutter",
    "line",
    "overview",
  ],
  "gitlens.statusBar.enabled": false,
  "gitlens.views.compare.enabled": true,
  "gitlens.views.compare.files.layout": "tree",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": false,
  "gitlens.views.lineHistory.location": "explorer",
  "gitlens.views.search.enabled": false,
  "gitlens.views.search.location": "gitlens",
  "indentRainbow.updateDelay": 1000,
  "javascript.format.semicolons": "remove",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveshare.increasedGuestLimit": true,
  "liveshare.showInStatusBar": "whileCollaborating",
  "markdown.extension.showExplorer": false,
  "rewrap.autoWrap.enabled": false,
  "rewrap.reformat": true,
  "rewrap.wholeComment": false,
  "ruby.intellisense": "rubyLocate",
  "ruby.useLanguageServer": false,
  "scm.defaultViewMode": "tree",
  "scm.diffDecorations": "all",
  "scm.diffDecorationsGutterVisibility": "hover",
  "scm.providers.visible": 0,
  "search.globalFindClipboard": true,
  "search.smartCase": true,
  "sort-imports.default-sort-style": "module",
  "stylint.run": "onSave",
  "svgviewer.showzoominout": true,
  "tableformatter.common.centerAlignedHeader": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.scrollback": 100000,
  "typescript.format.insertSpaceAfterCommaDelimiter": false,
  "typescript.format.semicolons": "remove",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "manual",
  "vsicons.dontShowNewVersionMessage": true,
  "vsicons.presets.hideExplorerArrows": true,
  "vsicons.presets.hideFolders": true,
  "vsicons.presets.jsOfficial": true,
  "vsicons.presets.jsonOfficial": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vtools.autoHideDelay": 50,
  "whiteviz.enabled": false,
  "whiteviz.visualizeOnlyIndentation": true,
  "window.closeWhenEmpty": false,
  "window.nativeTabs": true,
  "window.restoreWindows": "none",
  "window.title": "${rootName}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "NaturalContrast-HC",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.openPositioning": "right",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.showTabs": false,
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.list.keyboardNavigation": "filter",
  "workbench.settings.useSplitJSON": true,
  "workbench.sideBar.location": "right",
  "workbench.tree.indent": 16,
}
