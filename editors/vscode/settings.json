{
  // Multi-line settings. These can't be automatically sorted :(
  "[markdown]": {
    "rewrap.autoWrap.enabled": false,
  },
  "[plaintext]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4,
    "files.insertFinalNewline": false,
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop",
  },
  "blockhighlight.omit": [
    "typescriptreact",
    "plaintext",
  ],
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "css",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pub",
    "python",
    "restructuredtext",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml",
    "ruby",
  ],
  "cSpell.ignoreRegExpList": [],
  "cSpell.userWords": [
    "Asana",
    "Citus",
    "Crypted",
    "Dedupe",
    "Deduping",
    "Deno",
    "Depluralize",
    "Kuang",
    "Linkedin",
    "Lyft",
    "REENABLED",
    "Rubo",
    "Unhide",
    "Uninvitable",
    "Unnests",
    "Zhou",
    "abcdefghijklmnopqrstuvwxyz",
    "architected",
    "bezier",
    "bignum",
    "bools",
    "camelization",
    "camelize",
    "csrf",
    "debounced",
    "debouncing",
    "deduped",
    "denormalize",
    "divs",
    "eightvc",
    "elif",
    "encodeji",
    "eqnull",
    "esversion",
    "eval",
    "expnd",
    "gabe",
    "gmail",
    "groupby",
    "gsub",
    "hacky",
    "haml",
    "hellip",
    "icomoon",
    "introducee",
    "isatty",
    "lapras",
    "linkify",
    "linkifyjs",
    "lunala",
    "mktemp",
    "munge",
    "nbsp",
    "nokogiri",
    "nums",
    "opps",
    "orgs",
    "parallelize",
    "prettierrc",
    "rbenv",
    "rc",
    "rebases",
    "regioned",
    "repo",
    "repr",
    "rgba",
    "rstrip",
    "rsync",
    "rubocop",
    "serializable",
    "setex",
    "shrey",
    "sidekiq",
    "spammy",
    "styl",
    "tada",
    "uncomment",
    "ungrouped",
    "unindent",
    "unioned",
    "unlisten",
    "unmount",
    "unmounts",
    "unpersistent",
    "unsanitized",
    "unstaged",
    "upcase",
    "vals",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
  },
  "editor.rulers": [
    90,
  ],
  "quickOpenRelatedFiles.patternsToStrip": [
    "{EXTENSION}",
    "/[_\\.]?[sS]pec$/",
    "/[_\\.]?[tT]est$/",
    "/[_\\.]?[cC]component$/",
  ],
  "ruby.lint": {
    "fasterer": true,
    "reek": true,
    // "rubocop": true,
  },
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#9aa1",
    "scrollbarSlider.activeBackground": "#9aac",
    "scrollbarSlider.hoverBackground": "#9aa8",
  },

  // Single-line settings.
  "auto-comment-blocks.singleLineBlockOnEnter": true,
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.showCommandsInContextMenu": false,
  "bookmarks.useWorkaroundForFormatters": true,
  "breadcrumbs.enabled": true,
  "breadcrumbs.symbolPath": "off",
  "cSpell.allowCompoundWords": true,
  "cSpell.checkLimit": 30,
  "cSpell.showStatus": false,
  "debug.enableAllHovers": true,
  "debug.inlineValues": true,
  "debug.toolBarLocation": "docked",
  "deno.alwaysShowStatus": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoIndent": false,
  "editor.codeActionsOnSaveTimeout": 7000,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.seedSearchStringFromSelection": false,
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.formatOnSaveTimeout": 7000,
  "editor.gotoLocation.multiple": "gotoAndPeek",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "interval",
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.quickSuggestionsDelay": 50,
  "editor.renderIndentGuides": false,
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "mixed",
  "extensions.autoUpdate": true,
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autoRepositoryDetection": "subFolders",
  "git.countBadge": "off",
  "git.enableSmartCommit": true,
  "git.showPushSuccessNotification": true,
  "githubPullRequests.showInSCM": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultGravatarsStyle": "identicon",
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": false,
  "gitlens.views.compare.enabled": false,
  "gitlens.views.fileHistory.enabled": false,
  "gitlens.views.lineHistory.enabled": false,
  "gitlens.views.repositories.enabled": false,
  "gitlens.views.search.enabled": false,
  "indentRainbow.updateDelay": 1000,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveshare.codeLens": false,
  "liveshare.increasedGuestLimit": true,
  "liveshare.showInStatusBar": "whileCollaborating",
  "markdown.extension.showExplorer": false,
  "prettier.requireConfig": true,
  "prettier.tslintIntegration": true,
  "rewrap.autoWrap.enabled": false,
  "rewrap.reformat": true,
  "rewrap.wholeComment": false,
  "scm.providers.visible": 0,
  "search.collapseResults": "alwaysCollapse",
  "search.location": "panel",
  "stylint.run": "onSave",
  "tableformatter.common.centerAlignedHeader": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.scrollback": 100000,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "whiteviz.visualizeOnlyIndentation": true,
  "window.closeWhenEmpty": false,
  "window.openFilesInNewWindow": "default",
  "window.restoreWindows": "none",
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Cameo Dark+ (default dark)",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.openPositioning": "first",
  "workbench.editor.revealIfOpen": false,
  "workbench.editor.showTabs": true,
  "workbench.editor.tabCloseButton": "left",
  "workbench.list.keyboardNavigation": "filter",
  "workbench.settings.useSplitJSON": true,
  "workbench.sideBar.location": "left",
  "workbench.statusBar.feedback.visible": false,
  "workbench.tree.indent": 4,
  "files.associations": {
    "*.sublime-settings": "jsonc"
  },
}
