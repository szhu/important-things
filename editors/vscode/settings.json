{
    // Multi-line settings. These can't be automatically sorted :(
    "[plaintext]": {
        "editor.insertSpaces": false,
        "editor.tabSize": 4,
        "files.insertFinalNewline": false,
    },
    "cSpell.enabledLanguageIds": [
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pub",
        "python",
        "restructuredtext",
        "rust",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yml",
        "ruby",
    ],
    "cSpell.ignoreRegExpList": [
        "'s\\b",
        "\\bun",
        "\\bun\\b",
    ],
    "cSpell.userWords": [
        "Elif",
        "Gmail",
        "Lapras",
        "Orgs",
        "Unmount",
        "architected",
        "bignum",
        "camelization",
        "camelize",
        "debounced",
        "denormalize",
        "eval",
        "gabe",
        "gsub",
        "haml",
        "hellip",
        "linkify",
        "linkifyjs",
        "lunala",
        "nbsp",
        "nokogiri",
        "opps",
        "rstrip",
        "setex",
        "styl",
        "unindent"
    ],
    "editor.rulers": [
        90,
    ],
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "diff.header",
                "scope": "meta.diff, meta.diff.header",
                "settings": {
                    "foreground": "#75715E"
                }
            },
            {
                "name": "diff.deleted",
                "scope": "markup.deleted",
                "settings": {
                    "foreground": "#9A0003"
                }
            },
            {
                "name": "diff.inserted",
                "scope": "markup.inserted",
                "settings": {
                    "foreground": "#48A043"
                }
            },
            {
                "name": "diff.changed",
                "scope": "markup.changed",
                "settings": {
                    "foreground": "#B48B47"
                }
            }
        ]
    },
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "/Users/Sean/Code/x(|--.*)/.*",
                "cmd": "cd ${workspaceRoot} && test -e bin/util/rsync-to-clay && bin/util/rsync-to-clay -v && bin/util/macos-notify 'Visual Studio Code' 'rsync-to-clay' 'Sync completed'",
            }
        ]
    },
    "githubPullRequests.hosts": [
        {
            "host": "https://github.com",
            "username": "oauth",
            "token": "system"
        }
    ],
    "ruby.lint": {
        "fasterer": true,
        "reek": true,
        // "rubocop": true,
    },
    "sshfs.configs": [
        {
            "host": "ssh.ocf.berkeley.edu",
            "name": "seanzhu@ssh.ocf.berkeley.edu",
            "port": 22,
            "privateKeyPath": "/Users/Sean/.ssh/id_rsa",
            "root": "/home/s/se/seanzhu",
            "username": "seanzhu"
        },
        {
            "root": "/home/sean",
            "host": "x",
            "port": 20000,
            "username": "sean",
            "privateKeyPath": "/Users/Sean/.ssh/id_rsa",
            "name": "sean@x"
        }
    ],
    "tslint.autoFixOnSave": true,

    // Single-line settings.
    "auto-comment-blocks.singleLineBlockOnEnter": true,
    "breadcrumbs.enabled": true,
    "cSpell.allowCompoundWords": true,
    "cSpell.checkLimit": 500,
    "cSpell.showStatus": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoIndent": false,
    "editor.find.seedSearchStringFromSelection": false,
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.glyphMargin": true,
    "editor.insertSpaces": false,
    "editor.lineNumbers": "interval",
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestionsDelay": 300,
    "editor.renderIndentGuides": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollBeyondLastLine": false,
    "editor.snippetSuggestions": "top",
    "editor.tabSize": 2,
    "explorer.autoCollapse": false,
    "explorer.openEditors.visible": 20,
    "explorer.sortOrder": "mixed",
    "extensions.autoUpdate": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "git.autorefresh": false,
    "git.autoRepositoryDetection": "subFolders",
    "git.enableSmartCommit": true,
    "git.showPushSuccessNotification": true,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.statusBar.enabled": false,
    "markdown.extension.showExplorer": false,
    "prettier.requireConfig": true,
    "prettier.tslintIntegration": true,
    "python.pythonPath": "/usr/local/bin/python",
    "rewrap.wholeComment": false,
    "spotify.showMuteUnmuteVolumeButton": false,
    "spotify.showNextButton": false,
    "spotify.showPlayPauseButton": false,
    "spotify.showPreviousButton": false,
    "spotify.showSignOutButton": false,
    "tableformatter.common.centerAlignedHeader": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.scrollback": 100000,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.referencesCodeLens.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.closeWhenEmpty": false,
    "window.restoreFullscreen": true,
    "window.restoreWindows": "all",
    "window.title": "${rootName}${separator}${activeEditorMedium}",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.revealIfOpen": false,
    "workbench.editor.showTabs": false,
    "workbench.editor.tabCloseButton": "left",
    "workbench.iconTheme": "macos-classic-icons",
    "workbench.sideBar.location": "right",
    "workbench.statusBar.feedback.visible": false,
    "workbench.view.alwaysShowHeaderActions": true,
    "ungit.showButton": true,
    "workbench.list.openMode": "doubleClick",
    "tslint.alwaysShowRuleFailuresAsWarnings": true,
    "git.autofetch": true,
}
